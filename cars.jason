async function loadCars(){
  try {
    const resp = await fetch('https://orionfxservis.github.io/CK/main/cars.json');
    const cars = await resp.json();
    localStorage.setItem("cars", JSON.stringify(cars));
    renderCars(cars);
  } catch(e) {
    console.error("Failed to load cars from GitHub", e);
    renderCars([]);
  }
}
